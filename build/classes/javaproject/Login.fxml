<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="554.0" prefWidth="690.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaproject.LoginController">
    <stylesheets>
        <URL value="@login.css" />
    </stylesheets>
   <children>
      <Label layoutX="93.0" layoutY="30.0" prefHeight="63.0" prefWidth="505.0" text="Project GameOn">
         <font>
            <Font name="Castellar" size="48.0" />
         </font>
      </Label>
      <GridPane layoutX="130.0" layoutY="205.0" prefHeight="199.0" prefWidth="359.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="179.0" minWidth="10.0" prefWidth="146.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="223.0" minWidth="10.0" prefWidth="222.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="32.0" prefWidth="124.0" text="Username">
               <font>
                  <Font name="Berlin Sans FB" size="29.0" />
               </font>
            </Label>
            <PasswordField fx:id="txtPassword" prefHeight="41.0" prefWidth="218.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="20.0" />
               </font></PasswordField>
            <TextField fx:id="txtUserName" prefHeight="40.0" prefWidth="218.0" GridPane.columnIndex="1">
               <font>
                  <Font size="20.0" />
               </font></TextField>
            <Label text="Password" GridPane.rowIndex="1">
               <font>
                  <Font name="Berlin Sans FB" size="30.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#loginUser" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font name="Cooper Black" size="24.0" />
               </font>
            </Button>
         </children>
      </GridPane>
      <Label layoutX="130.0" layoutY="466.0" text="New player? ">
         <font>
            <Font name="Berlin Sans FB" size="28.0" />
         </font>
      </Label>
      <Button layoutX="296.0" layoutY="459.0" mnemonicParsing="false" onAction="#showRegistration" text="Create New Account">
         <font>
            <Font name="Cooper Black" size="24.0" />
         </font>
      </Button>
      <Label fx:id="lblResult" alignment="CENTER" layoutX="10.0" layoutY="148.0" prefHeight="31.0" prefWidth="658.0" text="Welcome to Login Page" textAlignment="CENTER">
         <font>
            <Font name="Berlin Sans FB" size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
